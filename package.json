{
  "name": "react-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:types && npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:types && npm run lint:prettier:fix && npm run lint:eslint:fix && npm run lint:stylelint:fix",
    "lint:types": "tsc --project ./tsconfig.json",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:prettier:single": "prettier --list-different",
    "lint:eslint": "eslint --format visualstudio --ext .js,.cjs,.ts,.tsx --report-unused-disable-directives --max-warnings 0 .",
    "lint:eslint:fix": "eslint --format visualstudio --ext .js,.cjs,.ts,.tsx --report-unused-disable-directives --max-warnings 0 --fix .",
    "lint:eslint:single": "eslint",
    "lint:stylelint": "stylelint 'src/**/*.{css,postcss}'",
    "lint:stylelint:fix": "stylelint 'src/**/*.{css,postcss}' --fix",
    "lint:stylelint:single": "stylelint"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^5.1.0",
    "postcss-nesting": "^12.1.2",
    "postcss-preset-env": "^9.5.11",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-tailwind-purgecss": "^0.3.3"
  }
}
